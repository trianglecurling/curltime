{
  "name": "timetocurl",
  "version": "1.0.0",
  "description": "Client and server software to run a timer for curling matches.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p . && webpack --config ./client/webpack.config.js",
    "watch:client": "webpack --config ./client/webpack.config.js --watch",
    "watch:server": "tsc -w -p .",
    "mon": "nodemon ./bin/index.js",
    "start": "node ./bin/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trianglecurling/timetocurl.git"
  },
  "keywords": [
    "curling",
    "timer",
    "curltime",
    "timetocurl",
    "typescript",
    "nodejs",
    "javascript",
    "socketio",
    "websockets"
  ],
  "author": "Trevor Gau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trianglecurling/timetocurl/issues"
  },
  "homepage": "https://github.com/trianglecurling/timetocurl#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "css-loader": "^5.2.6",
    "express": "^4.14.0",
    "fscreen": "^1.2.0",
    "lodash": "^4.17.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "mini-css-extract-plugin": "^2.1.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "tslib": "^1.7.1",
    "uuid": "^3.0.1",
    "webpack": "^5.43.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.69",
    "autoprefixer": "^10.2.6",
    "expect": "^1.20.2",
    "nodemon": "^2.0.9",
    "postcss-loader": "^6.1.1",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.7.2"
  }
}
